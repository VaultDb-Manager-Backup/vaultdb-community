{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "assets": []
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "src/api",
      "entryFile": "main",
      "sourceRoot": "src/api"
    },
    "worker": {
      "type": "application",
      "root": "src/worker",
      "entryFile": "main",
      "sourceRoot": "src/worker"
    }
  }
}
