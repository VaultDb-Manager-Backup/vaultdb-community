{
  "name": "vaultdb-community",
  "version": "0.1.0",
  "description": "Open source database backup and restore manager",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn build:core && yarn build:server && yarn build:cli",
    "build:core": "yarn workspace @vaultdb/core build",
    "build:server": "yarn workspace @vaultdb/server build",
    "build:cli": "yarn workspace @vaultdb/cli build",
    "start": "yarn workspace @vaultdb/server start",
    "start:dev": "yarn workspace @vaultdb/server start:dev",
    "start:worker": "yarn workspace @vaultdb/server start:worker",
    "lint": "eslint packages --ext .ts",
    "format": "prettier --write \"packages/**/*.ts\"",
    "test": "jest",
    "cli": "yarn workspace @vaultdb/cli cli"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VaultDb-Manager-Backup/vaultdb-community.git"
  },
  "keywords": [
    "database",
    "backup",
    "restore",
    "mysql",
    "postgresql",
    "mongodb",
    "s3"
  ],
  "author": "VaultDB Team",
  "bugs": {
    "url": "https://github.com/VaultDb-Manager-Backup/vaultdb-community/issues"
  },
  "homepage": "https://github.com/VaultDb-Manager-Backup/vaultdb-community#readme"
}
